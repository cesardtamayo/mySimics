#!/bin/bash
# this file will be overwritten by the project setup script
SIMICS_BASE_PACKAGE="/home/cesar/simics/simics-6.0.157"
export SIMICS_BASE_PACKAGE
if [ -f "/home/cesar/simics-projects/my_hello_world/.package-list" ]; then
    exec "/home/cesar/simics/simics-6.0.157/bin/lookup-file" --package-list "/home/cesar/simics-projects/my_hello_world/.package-list"  "$@"
else
    exec "/home/cesar/simics/simics-6.0.157/bin/lookup-file"  "$@"
fi
